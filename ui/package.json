{
  "name": "CRC-Auto-Minter",
  "version": "0.0.1",
  "main": "main/index.js",
  "type": "module",
  "scripts": {
    "electron": "cross-env NODE_ENV=development electron-forge start",
    "electron:dev": "cross-env NODE_ENV=development electronmon .",
    "start": "concurrently \"npm run electron:dev\" \"npm run react\"",
    "react": "cd renderer && npm run dev",
    "build": "cd renderer && npm run build && cd .. && npx electron-forge package",
    "make": "cd renderer && npm run build && cd .. && npx electron-forge make"
  },
  "keywords": [],
  "author": "Michal Jadach",
  "license": "All rights reserved",
  "description": "",
  "devDependencies": {
    "@electron-forge/cli": "^7.8.1",
    "@electron-forge/maker-deb": "^7.8.1",
    "@electron-forge/maker-squirrel": "^7.8.1",
    "@electron-forge/maker-zip": "^7.8.1",
    "concurrently": "^9.2.0",
    "cross-env": "^10.0.0",
    "electron": "^37.2.2",
    "electron-forge": "^5.2.4",
    "electron-prebuilt-compile": "^8.2.0",
    "electronmon": "^2.0.3"
  },
  "dependencies": {
    "dotenv": "^17.2.0",
    "electron-store": "^10.1.0"
  }
}
