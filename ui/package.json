{
  "name": "CRC-Auto-Minter",
  "version": "0.0.1",
  "main": "main/index.js",
  "type": "module",
  "scripts": {
    "electron": "cross-env NODE_ENV=development electron .",
    "electron:dev": "cross-env NODE_ENV=development electronmon . -w main",
    "start": "concurrently \"npm run electron:dev\" \"npm run react\"",
    "react": "cd renderer && npm run dev",
    "package": "cd renderer && npm run build && cd .. && npx electron-builder --config electron-builder.json --dir",
    "installer:win": "cd renderer && npm run build && cd .. && npx electron-builder --config electron-builder.json --win --x64",
    "installer:mac": "cd renderer && npm run build && cd .. && npx electron-builder --config electron-builder.json --mac --x64 --arm64"
  },
  "keywords": [],
  "author": "Michal Jadach",
  "license": "All rights reserved",
  "description": "",
  "devDependencies": {
    "concurrently": "^9.2.0",
    "cross-env": "^10.0.0",
    "electron": "^37.2.2",
    "electron-builder": "^26.0.12",
    "electron-prebuilt-compile": "^8.2.0",
    "electronmon": "^2.0.3"
  },
  "dependencies": {
    "@circles-sdk/sdk": "^0.29.0",
    "@cometh/connect-sdk-4337": "^1.0.13",
    "dotenv": "^17.2.0",
    "electron-store": "^10.1.0",
    "ethers": "^6.15.0",
    "viem": "^2.33.1"
  }
}
